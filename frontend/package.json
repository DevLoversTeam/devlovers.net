{
  "name": "frontend",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "seed:categories": "tsx db/seed-categories.ts",
    "seed:questions": "tsx db/seed-questions.ts",
    "seed:users": "tsx db/seed-users.ts",
    "parse": "tsx parse/parse.ts",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
